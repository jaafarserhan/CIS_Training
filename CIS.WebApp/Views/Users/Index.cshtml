@model IEnumerable<CIS.WebApp.User>


<h2>@ViewBag.Title</h2>

<p>
    <a href="~/Users/Create">Create New</a>
</p>
<table class="table">
    <tr>
        <th>
            <p>First Name</p>
        </th>
        <th>
            Middle Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            User name
        </th>
        <th>
            Email
        </th>
  
        <th>
            Primary Mobile
        </th>
        <th>
            Profile Picture
        </th>
        <th>
            Creation Date
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.FirstName
        </td>
        <td>
            @item.MiddleName
        </td>
        <td>
            @item.LastName
        </td>
        <td>
            @item.Username
        </td>
        <td>
            @item.Email
        </td>
        <td>
            @item.PrimaryMobile
        </td>
        <td>
            @item.ProfilePicture
        </td>
        <td>
            @item.CreationDate
        </td>
        <td>

            <a href="~/Users/Edit/@item.UserId">Edit</a> |
            <a href="~/Users/Delete/@item.UserId">Delete</a> | 
            <a href="~/Users/Details/@item.UserId">Details</a>

           
        </td>
    </tr>
}

</table>
